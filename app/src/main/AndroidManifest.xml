<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.intelligentparkingsystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".parking_slots"></activity>
        <activity
            android:name=".no_of_users"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".Alogged_in" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".user_login" />
        <activity android:name=".admin_login" />
        <activity android:name=".Register1" />
        <activity
            android:name=".Ulogged_In"
            android:theme="@style/AppTheme" />
        <activity android:name=".PasswordActivity" />
    </application>

</manifest>